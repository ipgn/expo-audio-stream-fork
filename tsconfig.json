{
    "compilerOptions": {
        "target": "ES2017",
        "module": "ESNext",
        "moduleResolution": "node",
        "lib": ["ES2017", "DOM"],
        "jsx": "react-native",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "declaration": true,
        "declarationMap": true
    },
    "include": ["src"],
    "exclude": ["node_modules", "build"]
}
